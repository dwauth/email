{
  "name": "@dwauth/email",
  "version": "0.1.1",
  "description": "dwAuth Library For Sending Authentication Emails.",
  "contributors": [
    "Jared Rice Sr. <jared@dwebs.io>",
    "Distributed Webs Project, LLC. <labs@distributedwebs.org>"
  ],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --unused --no-dev && ncu",
